{
  "type": "origins:multiple",
  "scale_overtime": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "scale set extraorigins:modify_size 0.65",
      "permission_level": 4
    },
    "interval": 20
  },
  "scale_callback": {
    "type": "origins:action_on_callback",
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set extraorigins:modify_size 0.65",
      "permission_level": 4
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set extraorigins:modify_size 0.65",
      "permission_level": 4
    },
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set extraorigins:modify_size 0.65",
      "permission_level": 4
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set 1",
      "permission_level": 4
    }
  },
  "name": "Owl Sized",
  "description": "You are a bit smaller than humans."
}